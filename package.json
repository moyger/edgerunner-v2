{
  "name": "edgerunner-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run backend:start\" \"vite\"",
    "dev:frontend-only": "vite",
    "backend:start": "cd backend && (python3 start.py || python start.py)",
    "backend:stop": "pkill -f 'python start.py' || true",
    "start": "npm run backend:start && npm run dev:frontend-only",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
